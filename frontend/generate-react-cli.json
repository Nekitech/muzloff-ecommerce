{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {

    "modules": {
      "customTemplates": {
        "component": "src/app/(components)/defaultTemplates/defModules/TemplateName.tsx",
        "style": "src/app/(components)/defaultTemplates/defModules/TemplateName.module.scss"
      },
      "path": "src/app/(components)/(modules)",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },

    "templates": {
      "customTemplates": {
        "component": "src/app/(components)/defaultTemplates/defTemplates/TemplateName.tsx",
        "style": "src/app/(components)/defaultTemplates/defTemplates/TemplateName.module.scss"
      },
      "path": "src/app/(components)/templates",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },

    "UI": {
      "customTemplates": {
        "component": "src/app/(components)/defaultTemplates/defTemplates/TemplateName.tsx",
        "style": "src/app/(components)/defaultTemplates/defTemplates/TemplateName.module.scss"
      },
      "path": "src/app/(components)/UI",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },

    "default": {
      "path": "src",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}